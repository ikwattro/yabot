{
    "name": "nopolabs/yabot",
    "description": "Yet Another Bot",
    "version": "0.0.1",
    "type": "package",
    "require": {
        "php": ">=5.5.0",
        "coderstephen/slack-client": "^0.2.5",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "~1.13",
        "symfony/dependency-injection": "^3.2",
        "symfony/config": "^3.2",
        "symfony/expression-language": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "nopolabs/mock-with-expectations": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Revel",
            "email": "dan@nopolabs.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nopolabs\\Yabot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nopolabs\\Yabot\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nopolabs/mock-with-expectations"
        }
    ]
}
